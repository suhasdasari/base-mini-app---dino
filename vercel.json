{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/.well-known/farcaster.json",
      "destination": "https://api.farcaster.xyz/miniapps/hosted-manifest/0199ac2b-065f-6f1e-2621-cc20d930d4ae",
      "permanent": false
    }
  ],
  "routes": [
    {
      "src": "/game.js",
      "dest": "/game.js"
    },
    {
      "src": "/style.css", 
      "dest": "/style.css"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
